{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./"
      ],
      "@interfaces/*": [
        "./interfaces/*"
      ],
      "@enums/*": [
        "./enums/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@css/*": [
        "./assets/css/*"
      ],
      "@images/*": [
        "./assets/images/*"
      ],
      "@layouts/*": [
        "./layouts/*"
      ],
      "@types/*": [
        "./types/*"
      ]
    }
  },
  "include": [
    ".nuxt/**/*",
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue"
  ],
  "extends": "./.nuxt/tsconfig.json"
}